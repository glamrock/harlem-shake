{
    "name": "Harlem Shake",
    "version": "0.90",
	"manifest_version": 2,
	"icons": {"16": "icon.png", "32": "icon.png", "48": "icon.png", "64": "icon.png", "128": "icon.png" },
	"description": "Make any website do the Harlem Shake!",
    "background": {
      "page": "background.html" 
   },
    "browser_action": {
	"default_icon": "icon.png",
    "default_title": "Harlem Shake"
    }
    "permissions": [
        "tabs", "http://*/*"
],
}
